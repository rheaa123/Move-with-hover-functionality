{
    "manifest_version": 3,
    "name": "Trial To-Do List Extension",
    "version": "1.0",
    "description": "A simple to-do list Chrome extension.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/catdog.png",
        "48": "icons/catdog.png",
        "128": "icons/catdog.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  